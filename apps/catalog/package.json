{
  "name": "@pokepoke/catalog",
  "version": "0.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "gen:mobile": "sb-rn-get-stories -c ./.storybook/mobile",
    "sb": "storybook dev -p 6006 --no-open -c ./.storybook/web",
    "sb:mobile": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED=true expo start --port=8082",
    "sb:ios": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED=true expo start --ios --port=8082",
    "sb:build": "storybook build -o ./.storybook/static -c ./.storybook/web",
    "ios": "expo run:ios --port=8082",
    "android": "expo run:android --port=8082",
    "check": "biome check --error-on-warnings --no-errors-on-unmatched",
    "check:fix": "bun run check --write",
    "storybook-generate": "sb-rn-get-stories"
  },
